{
  "$schema": "../chain.schema.json",
  "chain_name": "auramvp",
  "chain_id": "aura-mvp-1",
  "chain_type": "cosmos",
  "pretty_name": "AURA MVP Testnet",
  "status": "upcoming",
  "network_type": "testnet",
  "bech32_prefix": "aura",
  "daemon_name": "aurad",
  "node_home": "$HOME/.aura",
  "key_algos": ["secp256k1"],
  "slip44": 118,
  "fees": {
    "fee_tokens": [
      {
        "denom": "uaura",
        "fixed_min_gas_price": 0.025,
        "low_gas_price": 0.025,
        "average_gas_price": 0.05,
        "high_gas_price": 0.1
      }
    ]
  },
  "staking": {
    "staking_tokens": [
      {
        "denom": "uaura"
      }
    ]
  },
  "codebase": {
    "git_repo": "https://github.com/aura-blockchain/aura",
    "recommended_version": "v1.0.0-mvp",
    "compatible_versions": ["v1.0.0-mvp"],
    "binaries": {
      "linux/amd64": "https://artifacts.aurablockchain.org/mvp/v1.0.0-mvp/aurad-v1.0.0-mvp-linux-amd64",
      "linux/arm64": "https://artifacts.aurablockchain.org/mvp/v1.0.0-mvp/aurad-v1.0.0-mvp-linux-arm64",
      "darwin/amd64": "https://artifacts.aurablockchain.org/mvp/v1.0.0-mvp/aurad-v1.0.0-mvp-darwin-amd64",
      "darwin/arm64": "https://artifacts.aurablockchain.org/mvp/v1.0.0-mvp/aurad-v1.0.0-mvp-darwin-arm64"
    },
    "genesis": {
      "genesis_url": "https://artifacts.aurablockchain.org/mvp/genesis.json"
    }
  },
  "description": "AURA MVP release testnet with 12 essential modules for credential verification.",
  "peers": {
    "seeds": [
      {
        "id": "TBD",
        "address": "testnet-rpc.aurablockchain.org:10656",
        "provider": "AURA Foundation"
      }
    ],
    "persistent_peers": [
      {
        "id": "TBD",
        "address": "testnet-rpc.aurablockchain.org:10656",
        "provider": "AURA Foundation"
      },
      {
        "id": "TBD",
        "address": "testnet-rpc.aurablockchain.org:10756",
        "provider": "AURA Foundation"
      }
    ]
  },
  "apis": {
    "rpc": [
      {
        "address": "https://testnet-rpc.aurablockchain.org",
        "provider": "AURA Foundation"
      }
    ],
    "rest": [
      {
        "address": "https://testnet-api.aurablockchain.org",
        "provider": "AURA Foundation"
      }
    ],
    "grpc": [
      {
        "address": "testnet-grpc.aurablockchain.org:443",
        "provider": "AURA Foundation"
      }
    ]
  },
  "explorers": [
    {
      "kind": "AURA Explorer",
      "url": "https://testnet-explorer.aurablockchain.org",
      "tx_page": "https://testnet-explorer.aurablockchain.org/tx/${txHash}",
      "account_page": "https://testnet-explorer.aurablockchain.org/account/${accountAddress}"
    }
  ],
  "features": {
    "cosmwasm": {
      "enabled": true,
      "version": "0.45.0"
    }
  },
  "mvp_modules": {
    "cosmos_sdk": ["auth", "bank", "staking", "slashing", "distribution", "consensus", "upgrade", "params", "genutil"],
    "cosmwasm": ["wasm"],
    "aura_custom": ["identity", "vcregistry", "dataregistry", "compliance", "governance", "prevalidation"]
  }
}
