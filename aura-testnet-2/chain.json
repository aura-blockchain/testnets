{
  "$schema": "../chain.schema.json",
  "chain_name": "auratestnet2",
  "chain_id": "aura-testnet-2",
  "pretty_name": "AURA Testnet 2",
  "status": "live",
  "network_type": "testnet",
  "bech32_prefix": "auratest",
  "daemon_name": "aurad",
  "node_home": "$HOME/.aura",
  "key_algos": ["secp256k1"],
  "slip44": 118,
  "fees": {
    "fee_tokens": [
      {
        "denom": "uaura",
        "fixed_min_gas_price": 0.001,
        "low_gas_price": 0.001,
        "average_gas_price": 0.002,
        "high_gas_price": 0.003
      }
    ]
  },
  "staking": {
    "staking_tokens": [
      {
        "denom": "uaura"
      }
    ]
  },
  "codebase": {
    "git_repo": "https://github.com/aura-blockchain/aura",
    "recommended_version": "v0.1.0",
    "compatible_versions": ["v0.1.0"],
    "genesis": {
      "genesis_url": "https://raw.githubusercontent.com/aura-blockchain/testnets/main/aura-testnet-2/genesis.json"
    }
  },
  "apis": {
    "rpc": [
      {
        "address": "https://rpc.testnet.aurablockchain.org",
        "provider": "Aequitas Foundation"
      }
    ],
    "rest": [
      {
        "address": "https://api.testnet.aurablockchain.org",
        "provider": "Aequitas Foundation"
      }
    ]
  },
  "explorers": [
    {
      "kind": "AURA Explorer",
      "url": "https://explorer.testnet.aurablockchain.org",
      "tx_page": "https://explorer.testnet.aurablockchain.org/tx/${txHash}"
    }
  ]
}
